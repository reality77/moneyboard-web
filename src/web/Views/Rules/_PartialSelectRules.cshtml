@using web.Utils
@model web.Models.Rules.RulesSelectModel

<select id="@Model.HtmlFieldId" name="@Model.HtmlFieldName" onchange="@Model.OnChangeScript">
@foreach(var item in Model.ListItems) {
    @Html.Raw($"<option value='{item.Value}' {(item.Selected ? "selected" : "")}>{item.Text}</option>")
}
</select>
